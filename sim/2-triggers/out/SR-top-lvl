    %inv;
    %store/vec4 v0x563529e97f70_0, 0, 1;
    %jmp T_0.0;
    %vpi_call 2 17 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563529e97f70_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 20 "$display", "SUCCESS!" {0 0 0};
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_0;
